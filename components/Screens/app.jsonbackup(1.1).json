{
    "expo": {
      "name": "mobileapp",
      "slug": "mobileapp",
      "version": "1.0.0",
      "orientation": "portrait",
      "scheme": "myapp",
      "userInterfaceStyle": "automatic",
      "newArchEnabled": true,
      "ios": {
        "supportsTablet": true
      },
      "android": {
        "permissions": [
          "ACCESS_FINE_LOCATION",
          "CAMERA",
          "READ_EXTERNAL_STORAGE",
          "WRITE_EXTERNAL_STORAGE"
        ],
        "icon": "./assets/images/icon.png",
        "adaptiveIcon": {
          "foregroundImage": "./assets/images/adaptive-icon.png",
          "backgroundColor": "#ffffff"
        },
        "package": "com.hamzawaqasch.mobileapp"
      },
      "web": {
        "bundler": "metro",
        "output": "static",
        "favicon": "./assets/images/favicon.png"
      },
      "plugins": [
        [
          "expo-splash-screen",
          {
            "image": "./assets/images/splash-icon.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
          }
        ]
      ],
      "experiments": {
        "typedRoutes": true
      },
      "extra": {
        "eas": {
          "projectId": "410aabf1-0a0d-4667-9ef2-39b0bd270ba9"
        }
      }
    }
  }
  